//Maximize Toys solution
class Solution {
    static int toyCount(int N, int K, int arr[]) {
     
        Arrays.sort(arr);
        int count=0;
        for(int i=0;i<arr.length;i++)
        {
            K-=arr[i];
            if(K>=0)
                count++;
            else
                break;
            
        }
        return count;
    }
}
