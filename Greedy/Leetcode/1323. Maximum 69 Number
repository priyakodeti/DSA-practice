//Leetcode (Easy)
//1323. Maximum 69 Number
class Solution {
    public int maximum69Number (int num) {
        char[] ch=String.valueOf(num).toCharArray();

        for(int i=0;i<ch.length;i++)
            {
                if(ch[i]!='9')
                {
                    ch[i]='9';
                    break;
                }
            }
        return Integer.valueOf(new String(ch));
    }
}
