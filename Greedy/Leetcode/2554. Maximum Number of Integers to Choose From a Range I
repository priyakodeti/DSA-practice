//Leetcode (Medium)
//2554. Maximum Number of Integers to Choose From a Range I
class Solution {
    public int maxCount(int[] banned, int n, int maxSum) {
        HashSet<Integer> set=new HashSet<>();
        for(int i:banned)
            set.add(i);

        int sum=0,c=0;
        for(int i=1;i<=n;i++)
        {
            if(set.contains(i))
                continue;
           if((sum+i)<=maxSum)
           {
            c++;
            sum+=i;
           }
           else
                break;
           
        }
        return c;
    }
}
