//Leetcode (Medium)
//1833. Maximum Ice Cream Bars
class Solution {
    public int maxIceCream(int[] costs, int coins) {
        Arrays.sort(costs);
        int res=0;
        for(int i=0;i<costs.length;i++)
        {
            if(costs[i]>coins || coins==0)
                break;
            else 
            {
                res++;
                coins-=costs[i];
            }
        }
        return res;
    }
}
