//Max Sum Subarray of size K
//Difficulty level: Easy

class Solution {
    public int maxSubarraySum(int[] arr, int k) {
       
        int maxSum=0,sum=0,l=0,r=0;
        
        while(r<arr.length)
        {
            sum+=arr[r];
            if(r-l+1>k)
            {
                sum-=arr[l];
                l++;
            }
            maxSum=Math.max(sum,maxSum);
            r++;
        }
        return maxSum;
    }
}
