class Solution {
    public int uniquePaths(int m, int n) {
        
        if(m==1 || n==1)
            return 1;
        else if (m<=0 || n<=0)
            return 0;
        else
        {
            int up=uniquePaths(m-1,n);
            int left=uniquePaths(m,n-1);
            return up+left;
        }

    }
}
